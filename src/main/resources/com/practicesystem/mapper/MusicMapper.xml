<mapper namespace="com.practicesystem.mapper.MusicMapper">

    <select id="findAll" resultType="com.practicesystem.pojo.Music">
        select * from songs.tb_songs
    </select>
    <delete id="deleteById">
        delete from songs.tb_songs where id=#{id}
    </delete>
